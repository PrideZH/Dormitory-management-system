<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pengfu.dao.ManagerMapper">

	<select id="selectByUsername" resultType="manager">
		select * from manager where username = #{username}
	</select>
	
	<select id="selectAll" resultType="manager">
		select * from manager
	</select>
	
	<!-- 添加 -->
	<insert id="insert" parameterType="manager">
		insert into manager(bid, mid)
		values(#{name},#{buildingId})
	</insert>
	
</mapper>